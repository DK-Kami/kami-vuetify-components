{
  "name": "kami-vuetify-components",
  "version": "0.6.3",
  "description": "Library of Vuetify components",
  "main": "dist/kami-vuetify-components.umd.js",
  "module": "dist/kami-vuetify-components.esm.js",
  "unpkg": "dist/kami-vuetify-components.min.js",
  "browser": {
    "./sfc": "src/index.vue"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/kami-vuetify-components.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/kami-vuetify-components.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/kami-vuetify-components.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KamiMeow/kami-vuetify-components.git"
  },
  "keywords": [
    "Vue",
    "Vue.js",
    "Vuetify",
    "components"
  ],
  "author": "Kami-san",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KamiMeow/kami-vuetify-components/issues"
  },
  "homepage": "https://github.com/KamiMeow/kami-vuetify-components#readme",
  "dependencies": {
    "vuetify": "^2.3.8"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^15.0.0",
    "rollup": "^2.26.3",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  }
}
